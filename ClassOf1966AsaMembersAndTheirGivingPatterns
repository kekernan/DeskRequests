SELECT ID, FULL_NAME, CLASS, COLLEGE, CFAE_CAT,
      (CASE
          WHEN DCLUB_CYR IS NOT NULL AND DCLUB_PYR IS NOT NULL AND DCLUB_3YR IS NOT NULL AND DCLUB_4YR IS NOT NULL AND DCLUB_5YR IS NOT NULL AND DCLUB_6YR IS NOT NULL
          THEN '1 Gave Every FY Including Current'
          WHEN DCLUB_PYR IS NOT NULL AND DCLUB_3YR IS NOT NULL AND DCLUB_4YR IS NOT NULL AND DCLUB_5YR IS NOT NULL AND DCLUB_6YR IS NOT NULL
          THEN '2 Gave Every FY'
          WHEN (DCLUB_PYR IS NOT NULL AND DCLUB_3YR IS NOT NULL AND DCLUB_4YR IS NOT NULL AND DCLUB_5YR IS NOT NULL ) OR (DCLUB_3YR IS NOT NULL AND DCLUB_4YR IS NOT NULL AND DCLUB_5YR IS NOT NULL AND DCLUB_6YR IS NOT NULL)
                OR (DCLUB_PYR IS NOT NULL AND DCLUB_3YR IS NOT NULL  AND DCLUB_5YR IS NOT NULL AND DCLUB_6YR IS NOT NULL)
          THEN '3 Gave four out of five'
          WHEN (DCLUB_PYR IS NOT NULL AND DCLUB_3YR IS NOT NULL AND DCLUB_4YR IS NOT NULL) OR (DCLUB_4YR IS NOT NULL AND DCLUB_5YR IS NOT NULL AND DCLUB_6YR IS NOT NULL) OR
               (DCLUB_PYR IS NOT NULL  AND DCLUB_4YR IS NOT NULL  AND DCLUB_6YR IS NOT NULL) OR (DCLUB_PYR IS NOT NULL AND DCLUB_5YR IS NOT NULL AND DCLUB_6YR IS NOT NULL) or
               (DCLUB_3YR IS NOT NULL AND DCLUB_5YR IS NOT NULL AND DCLUB_6YR IS NOT NULL) OR (DCLUB_3YR IS NOT NULL AND DCLUB_4YR IS NOT NULL AND  DCLUB_6YR IS NOT NULL) OR
               (DCLUB_PYR IS NOT NULL AND DCLUB_4YR IS NOT NULL AND DCLUB_5YR IS NOT NULL )
          THEN '4 Gave three out of five'
          WHEN (DCLUB_5YR IS NOT NULL AND DCLUB_6YR IS NOT NULL) OR (DCLUB_3YR IS NOT NULL AND DCLUB_4YR IS NOT NULL ) OR (DCLUB_PYR IS NOT NULL AND DCLUB_3YR IS NOT NULL) OR (DCLUB_4YR IS NOT NULL AND DCLUB_6YR IS NOT NULL)
          THEN '5 Gave two out of five'
          ELSE '6 Gave once'
       END) AS Category,
       DCLUB_CYR,DCLUB_PYR, DCLUB_3YR
       ,DCLUB_4YR,DCLUB_5YR,
       DCLUB_6YR

FROM DONOR
WHERE Class = 1966
AND CFAE_CAT = 'ALMB'
AND DEAD_IND IS NULL
AND(DCLUB_PYR IS NOT NULL OR DCLUB_PWEEK IS NOT NULL OR DCLUB_3YR
       IS NOT NULL OR DCLUB_4YR IS  NOT NULL OR DCLUB_5YR IS NOT NULL OR
       DCLUB_6YR IS NOT NULL)
ORDER BY Category, SORT_NAME       
