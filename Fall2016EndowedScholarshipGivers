select DONOR.ID "DONOR ID",
       DONOR.SORT_NAME,
       AXVAWRD.AXVAWRD_ID "DONEE ID",
       AXVAWRD.AXVAWRD_NAME,
       DONEE.CLASS,
       AXVAWRD.AXVAWRD_DESG
  from DONOR DONOR,
       DONOR DONEE,
       ADRDIDS,
       STVTERM,
       AXVAWRD
where ( DONOR.PIDM = ADRDIDS.ADRDIDS_PIDM
         and ADRDIDS.ADRDIDS_DESG = AXVAWRD.AXVAWRD_DESG
         and AXVAWRD.AXVAWRD_TERM_CODE = STVTERM.STVTERM_CODE
         and AXVAWRD.AXVAWRD_PIDM = DONEE.PIDM)
and (stvterm_code = 201540)  
and DONOR.ID IN (
) AND AXVAWRD_DESG <> '2200'
--and EXISTS(SELECT 'x'
--         FROM ADRATTR
--        WHERE ADRATTR.ADRATTR_DESG = AXVAWRD_DESG
--        and ADRATTR.ADRATTR_DATR_CODE in ('SCHO','SCEV','STWC','BTEV'))
ORDER BY SORT_Name
